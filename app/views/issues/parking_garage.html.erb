<html>
<head>
<style>

.trow1:hover { background: #d7d7d7; }
</style>
</head>
<body>
	<center>
		<h2>Parking Garage issues </h2>
	</center>
<%#= link_to "Back to Issues List", project_issues_path(@project.id) %>
	<hr/>
	<table width=100%>
	<tr bgcolor=#f4f4f4 width=100%>
		<th>Id</th>
		<th>Tracker</th>
  		<th>Status</th>
		<th>Priority</th>
		<th>Subject</th>
		<th>Updated</th>
		<th></th>
	</tr>
	<% @parking_issues.each do |p| %>
	
	<tr class="trow1">
		<td align=center><%= p.id %></td>
		<td align=center><%= p.tracker %></td>
		<td align=center><%= p.status %></td>
		<td align=center><%= p.priority %></td>
		<td align=center><%= link_to p.subject, issue_path(p.id) %></td>
		<td align=center><%= p.updated_on.to_date %></td>
		<td><%= link_to "update", edit_issue_path(p.id) %></td>
	</tr>
	<% @project3 = p.project_id %>
	<% end %>
	</table>
<hr/>
<%= link_to "Back to issues list ", project_issues_path(@project3) %>
</body>
</html>
