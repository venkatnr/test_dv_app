<table>
<%#= User.current.lastname %>
	<h2> <%= @iteration.name %> </h2>
		<p><b>List of Userstories</b></p>
	
		<tr width="1000px">
			<th width ="200px">Name</th>
			<th width ="200px">Disposition</th>
			<th width ="100px">Customer</th>
			<th width ="100px">Tracker</th>
			<th width ="100px">Status</th>
			<th width ="100px">Priority</th>
			<th width ="100px">Order</th>
	
			
		</tr>
              <% if @project.name == "Abseence data" %>
		<%@iteration.story.each do |st| %>
			<% if st.name.capitalize == User.current.lastname %>
			<tr width="1000px" align="center">
				<td width ="200px">

					<%=  link_to st.name, project_iteration_story_path(@project.id,@iteration.id, st.id) %>
				</td>
				<td width ="200px">

					<%= st.disposition %>
				</td>
				<td width ="200px">

					<%=   st.customer %>
				</td>
				<td width ="200px">

					<%=   st.tracker %>
				</td>
				<td width ="200px">

					<%=   st.status %>
				</td>
				<td width ="200px">

					<%=   st.priority %>
				</td>
				<td width ="200px">

					<%=  st.order %>
				</td>
	
				
				<td width ="200px">

					<%=  link_to "Update", edit_project_iteration_story_path(@project.id, @iteration.id, st.id) %>
				</td>
				
			</tr>
			<% end %>
		 <% end %>
                <% else %>
		<%@iteration.story.each do |st| %>
		<tr width="1000px" align="center">
				<td width ="200px">

					<%=  link_to st.name, project_iteration_story_path(@project.id,@iteration.id, st.id) %>
				</td>
				<td width ="200px">

					<%= st.disposition %>
				</td>
				<td width ="200px">

					<%=   st.customer %>
				</td>
				<td width ="200px">

					<%=   st.tracker %>
				</td>
				<td width ="200px">

					<%=   st.status %>
				</td>
				<td width ="200px">

					<%=   st.priority %>
				</td>
				<td width ="200px">

					<%=  st.order %>
				</td>
	
				
				<td width ="200px">

					<%=  link_to "Update", edit_project_iteration_story_path(@project.id, @iteration.id, st.id) %>
				</td>
				
			</tr>
		 <% end %>
		<% end %>
	    
</table>



<%if User.current.admin? %>
<%= link_to "create story" , new_project_iteration_story_path(@project.id, @iteration.id) %>
<%end %>

